{
	"name": "Audio EQ",
	"description": "HTML5 Audio Equalizer for Chrome",
	"version": "0.3.2.2",
	"manifest_version": 2,
	"permissions": ["tabs", "tabCapture", "activeTab", "storage", "contextMenus", "webRequest", "webRequestBlocking", "<all_urls>"],
	"background": {
		"scripts": ["common/const.js", "common/icon.js", "background/background.js"]
	},
	"browser_action": {
		"default_title": "Audio EQ",
		"default_icon": "128.png",
		"default_popup": "popup/popup.html"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"css": [],
			"js": ["common/const.js", "page/page.js"],
			"run_at": "document_start",
			"all_frames": true
		}
	],
	"homepage_url": "http://lab.ejci.net/Chrome-Audio-EQ/",
	"icons": {
		"16": "16.png",
		"48": "48.png",
		"128": "128.png",
		"256": "256.png"
	}
}